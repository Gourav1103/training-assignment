[
    {
      "operation": "modify-overwrite-beta",
      "spec": {
        "users": {
          "*": {
            "address?": "=concat(@(1,address.street),', ',@(1,address.city),', ', @(1,address.state),' ', @(1,address.zip))"
          }
        }
      }
    },
    {
      "operation": "shift",
      "spec": {
        "users": {
          "*": {
            "name": "users[&1].Name",
            "address": "users[&1].Address",
            "hobbies": "users[&1].Hobbies"
          }
        }
      }
    },
    {
      "operation": "shift",
      "spec": {
        "users": {
          "*": {
            "*": "[&1].&",
            "Hobbies": {
              "*": {
                "@(name)": "[&3].Hobbies"
              }
            }
          }
        }
      }
    }
    ]